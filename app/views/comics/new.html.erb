<%= form_with model: @comic, url:comics_path, local: true do |form| %>
      <%= form.text_field :name, placeholder: "name" %>
      <%#= form.text_field :image, placeholder: "Image Url" %>
      <%= form.text_area :genre_id, placeholder: "ジャンル" %>
      <%= form.text_area :magazine_id, placeholder: "雑誌" %>
      <%= form.text_field :author, placeholder: "作者名" %>
      <%= form.text_field :number, placeholder: "巻数" %>
      <%= form.text_field :end, placeholder: "完" %>
      <%= form.submit "SEND" %>
      <% end %>